<!DOCTYPE html>
<html>

<head>
  <%= csrf_meta_tags %>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge">
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <title></title>
  <!-- Favicon-->
  <link rel="icon" href="favicon.ico" type="image/x-icon">

  <!-- Google Fonts -->
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag "application-print", :media => "print" %>
  <%= javascript_include_tag 'charts' %>
</head>

<body class="theme-cyan">
<!-- Page Loader -->
<div class="page-loader-wrapper">
  <div class="loader">
    <div class="preloader">
      <div class="spinner-layer pl-red">
        <div class="circle-clipper left">
          <div class="circle"></div>
        </div>
        <div class="circle-clipper right">
          <div class="circle"></div>
        </div>
      </div>
    </div>
    <p>Please wait...</p>
  </div>
</div>
<!-- #END# Page Loader -->
<!-- Top Bar -->
<nav class="navbar">
  <div class="container-fluid">
    <div class="navbar-header">
      <a href="javascript:void(0);" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false"></a>
      <a href="javascript:void(0);" class="bars"></a>
      <%= link_to root_path do %>
          <%= image_tag 'fmoh.png' %>
      <% end %>
    </div>
    <%= render partial: 'menus/top_menu' %>
  </div>
</nav>
<!-- #Top Bar -->
<section>
  <!-- Left Sidebar -->
  <aside id="leftsidebar" class="sidebar">

    <!-- Menu -->
    <div class="menu">
      <ul class="list">
        <li class="header">MAIN NAVIGATION</li>
        <%= render partial: 'menus/biomedical_engineer' %>
        <%= render partial: 'menus/biomedical_head' %>
        <%= render partial: 'menus/department' %>
      </ul>
    </div>
    <!-- #Menu -->
    <!-- Footer -->

    <!-- #Footer -->
  </aside>
  <!-- #END# Left Sidebar -->
</section>

<section class="content">
  <div class="container-fluid">
    <div class="row clearfix">
        <%= render partial: 'shared/flash_messages' %>
      <% unless request.env['PATH_INFO'] == root_path %>
      <ol class="breadcrumb breadcrumb-bg-cyan align-right">
        <%= render_breadcrumbs tag: 'li', separator: "" %>
      </ol>
      <% end %>
        <%= yield :header %>
        <div class="card">
          <%= yield %>
        </div>
    </div>
  </div>
</section>
<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</body>
</html>