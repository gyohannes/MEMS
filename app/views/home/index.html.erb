<% if current_user.is_role(Constants::BIOMEDICAL_HEAD) || current_user.is_role(Constants::BIOMEDICAL_ENGINEER) %>
    <%= render 'dashboards/admin' %>
<% elsif current_user.is_role(Constants::DEPARTMENT) %>
    <%= render 'dashboards/department' %>
<% elsif current_user.is_role(Constants::SUPPLIER) %>
    <%= render 'dashboards/institution' %>
<% end %>