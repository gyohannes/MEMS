<div class="header">
<h2>
  Spare Part Requests
</h2>
</div>
<div class="body table-responsive">
  <table class="table table-bordered table-striped table-hover dataTable js-exportable">
  <thead>
    <tr>
      <th>Organization structure</th>
      <th>Facility</th>
      <th>Spare part name</th>
      <th>Model</th>
      <th>Volts ampere</th>
      <th>Power requirement</th>
      <th>Quantity</th>
      <th>Requested to</th>
      <th>Requested by</th>
      <th>Request date</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @spare_part_requests.each do |spare_part_request| %>
      <tr>
        <td><%= spare_part_request.organization_structure %></td>
        <td><%= spare_part_request.facility %></td>
        <td><%= spare_part_request.spare_part_name %></td>
        <td><%= spare_part_request.model %></td>
        <td><%= spare_part_request.volts_ampere %></td>
        <td><%= spare_part_request.power_requirement %></td>
        <td><%= spare_part_request.quantity %></td>
        <td><%= spare_part_request.requested_to %></td>
        <td><%= spare_part_request.requested_by %></td>
        <td><%= spare_part_request.request_date %></td>
        <td>
          <%= link_to 'Details', spare_part_request %> |
          <%= link_to 'Edit', edit_spare_part_request_path(spare_part_request) %> |
        <%= link_to 'Delete', spare_part_request, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
  <hr/>
<%= link_to 'New Spare Part Request', new_spare_part_request_path %>
</div>